<Window x:Class="SchoolWhiteWings.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchoolWhiteWings"
        mc:Ignorable="d"
        Title="Школа белые крылья" Height="760" Width="1280">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
        <Frame Background="#FAB7A4"/>
        <Image Source="../Resources/Images/logo.png"
               HorizontalAlignment="Left" Width="100"/>

        <Frame Grid.Row="1" x:Name="Log_frame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
